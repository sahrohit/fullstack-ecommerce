{
	"private": true,
	"scripts": {
		"build": "turbo run build --no-daemon",
		"clean": "turbo run clean --no-daemon",
		"dev": "turbo run dev --no-daemon --parallel --no-cache --continue --filter=admin --filter=api --filter=storefront",
		"dev:all": "turbo run dev --parallel --no-cache --continue",
		"format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"lint": "turbo run lint --no-daemon",
		"test": "turbo run test",
		"gen-env": "turbo run gen-env --no-daemon",
		"prepare": "husky install",
		"codegen": "turbo run codegen --no-daemon"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.5.1",
		"@commitlint/config-conventional": "^17.4.4",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.0",
		"prettier": "^2.7.1",
		"turbo": "^1.9.1"
	},
	"packageManager": "pnpm@7.15.0"
}
