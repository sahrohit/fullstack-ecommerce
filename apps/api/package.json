{
	"name": "api",
	"version": "0.0.1",
	"description": "Awesome project developed with TypeORM.",
	"private": true,
	"scripts": {
		"build": "tsup src/index.ts --format cjs",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
		"dev": "tsup src/index.ts --format cjs --watch --onSuccess \"node dist/index.js\"",
		"lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
		"start": "node dist/index.js",
		"gen-env": "gen-env-types .env -o src/env.d.ts -e .",
		"format:check": "prettier --check .",
		"format:write": "prettier --write .",
		"lint:check": "eslint .",
		"lint:fix": "eslint --fix .",
		"typeorm": "typeorm-ts-node-commonjs",
		"typeorm:create": "typeorm-ts-node-commonjs migration:create",
		"typeorm:generate": "typeorm-ts-node-commonjs migration:generate -d src/data-source.ts src/migration/Migration",
		"typeorm:migrate": "npx typeorm-ts-node-commonjs migration:run -d src/data-source",
		"typeorm:revert": "npx typeorm-ts-node-commonjs migration:revert -d src/data-source"
	},
	"devDependencies": {
		"@swc/core": "^1.2.114",
		"@types/connect-redis": "^0.0.18",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/express-session": "^1.17.4",
		"@types/ioredis": "^4.28.10",
		"@types/joi": "^17.2.3",
		"@types/node": "^17.0.40",
		"@types/nodemailer": "^6.4.4",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.27.1",
		"@typescript-eslint/parser": "^5.27.1",
		"eslint": "^8.17.0",
		"eslint-config-prettier": "^8.5.0",
		"gen-env-types": "^1.3.4",
		"nodemon": "^2.0.16",
		"prettier": "^2.6.2",
		"ts-node": "10.8.1",
		"tsup": "^6.2.3",
		"eslint-config-custom-server": "workspace:*",
		"typescript": "4.7.3",
		"tsconfig": "workspace:*"
	},
	"dependencies": {
		"apollo-server-express": "^3.8.2",
		"argon2": "^0.28.5",
		"connect-redis": "^6.1.3",
		"cors": "^2.8.5",
		"dotenv-safe": "^8.2.0",
		"express": "^4.18.1",
		"express-session": "^1.17.3",
		"helmet": "^5.1.0",
		"joi": "^17.6.0",
		"graphql": "^15.8.0",
		"nodemailer": "^6.7.5",
		"reflect-metadata": "^0.1.13",
		"pg": "^8.10.0",
		"ioredis": "^4.28.5",
		"type-graphql": "^1.1.1",
		"typeorm": "0.3.6",
		"uuid": "^8.3.2"
	}
}
