{
	"extends": "tsconfig/nextjs.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components/*": ["./components/*"],
			"@generated/*": ["./generated/*"],
			"@graphql/*": ["./graphql/*"],
			"@pages/*": ["./pages/*"],
			"@public/*": ["./public/*"],
			"@styles/*": ["./styles/*"]
		}
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
	"exclude": ["node_modules"]
}
